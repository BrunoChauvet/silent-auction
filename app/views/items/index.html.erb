<%= render partial: 'common/bidding_header', locals: {back_link: user_group_users_path(user_group_id: @user.user_group.id)} %>

<div id="items" class="container">
  <% @items.each do |item| %>
    <div class="col-md-12">
      <%= link_to "#{item.code} #{item.name}", user_item_path(user_id: params[:user_id], id: item.id) %>
    </div>
  <% end %>
</div>
